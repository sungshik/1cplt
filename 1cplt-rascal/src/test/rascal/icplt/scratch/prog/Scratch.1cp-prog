role @alice(i: number, foo: number, b: boolean, xyz: number[], s: string)
{
  main:
    i := [5, 6][0] ?? 7 ;
    xyz := xyz.slice(1) ;
    b := xyz.length == 0 ;
    xyz := [7].concat(xyz) ;
    i := 5 ;
    i -> @bob.bar |> {
      i := i + 1 ;
      i == true -> a.b |> {
        if b && false then
          i := 0
        else
          foo := 90
      }
    } ;

    i := 0 ;
    foreach<number> x in [5, 6, 7] do
      i := i + x ;

    s := "";
    foreach<string> x in ["foo"] do
      foreach<string> y in ["bar", "baz"] do
        s := s + x + y + " "
}

role @bob(i: number, bar: number, a: @alice)

process @alice[5](6, 8, true, [], "")
process @bob(8, 9, @alice[5])
