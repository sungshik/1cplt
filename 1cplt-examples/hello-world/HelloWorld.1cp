role @alice(i: number, n: number) {
  main:
    \save ;
    i := 0 ;
    while
      i < n
    do {
      inc ;
      \ping i*500 ;
      "hello, world" -> @bob.s |>
        \echo s
    } ;
    \load ;
    \echo {i: i, n: n}

  inc:
    i := i + 1
}

role @bob(s?: string)

process @alice(i = 0, n = 5)
process @bob()

